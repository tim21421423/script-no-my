spawn(function()
local player = game.Players.LocalPlayer
	local tool
	for i,v in player:GetDescendants() do
		if v.Name == "SyncAPI" then
			tool = v.Parent
		end
	end
	for i,v in game.ReplicatedStorage:GetDescendants() do
		if v.Name == "SyncAPI" then
			tool = v.Parent
		end
	end
	--craaa
	local remote = tool.SyncAPI.ServerEndpoint
    print(remote)
function refunc(chr)
	local args = {
		[1] = "Remove",
		[2] = {
			[1] = chr:FindFirstChild("Head")
		}
	}
	remote:InvokeServer(unpack(args))
end
function onchr(chr)
		local lastHealth = chr:WaitForChild"Humanoid".Health
		chr:WaitForChild"Humanoid".Changed:Connect(function(h2)if h2 ~= "Health" then return end local h = chr.Humanoid.Health if lastHealth < h then lastHealth = h return end lastHealth = h if h > 0.2 then return end warn'war is not an option' refunc(chr)end)
end
